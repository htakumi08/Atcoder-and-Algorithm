   "source": [
    "# B\n",
    "N = int(input())\n",
    "a = [int(input()) for _ in range(N)]\n",
    "\n",
    "count = 0\n",
    "cur = 1\n",
    "for _ in range(N):\n",
    "    # ボタン2が光ってる場合、終了\n",
    "    if cur == 2:\n",
    "        print(count)\n",
    "        break\n",
    "\n",
    "    # 次のボタンに移動\n",
    "    cur = a[cur-1]\n",
    "    count += 1\n",
    "\n",
    "if cur != 2:\n",
    "    print(-1)"
   ]
  }
