    "#B\n",
    "n,a,b = map(int, input().split())\n",
    "def func(x):\n",
    "    sum_ = 0\n",
    "    while x > 0:\n",
    "        sum_ += x%10\n",
    "        x //= 10\n",
    "    return sum_\n",
    "\n",
    "total = 0\n",
    "for i in range(1, n+1):\n",
    "    sum_ = func(i)\n",
    "    if a <= sum_ <= b:\n",
    "        total += i\n",
    "print(total)"
   ]
