    "#B\n",
    "S = input()\n",
    "acgt = 'ACGT'\n",
    "\n",
    "res = 0\n",
    "count = 0\n",
    "for i in S:\n",
    "    if i in acgt:\n",
    "        count += 1\n",
    "    else:\n",
    "        res = max(res, count)\n",
    "        count = 0\n",
    "\n",
    "# ループ後も更新\n",
    "res = max(res, count)\n",
    "print(res)\n"
   ]
