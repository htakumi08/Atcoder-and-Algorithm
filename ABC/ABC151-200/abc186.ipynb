    "# C-o\n",
    "N = int(input())\n",
    "\n",
    "cnt = 0\n",
    "for i in range(1,N+1):\n",
    "    if \"7\" in str(i) or \"7\" in str(oct(i)):\n",
    "        cnt += 1\n",
    "\n",
    "print(N-cnt)"
