   "source": [
    "#B\n",
    "S = input()\n",
    "n = len(S)\n",
    "\n",
    "if S == S[::-1]:\n",
    "    ans = n\n",
    "else:\n",
    "    ans = 1\n",
    "    \n",
    "for i in range(n):\n",
    "    for j in range(i+1,n):\n",
    "        T = S[i:j]\n",
    "        if T == T[::-1]:\n",
    "            ans = max(ans,len(T))\n",
    "print(ans)"
   ]
  }
