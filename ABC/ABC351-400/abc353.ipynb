    "# A-o\n",
    "N = int(input())\n",
    "H = list(map(int,input().split()))\n",
    "\n",
    "ans = -1\n",
    "for i in range(1,N):\n",
    "    if H[0] < H[i]:\n",
    "        ans = i+1\n",
    "        break\n",
    "\n",
    "print(ans)"
